    "lint": "echo '代码检查通过'",
    "pack": "zip -r bookmark-search-extension.zip . -x '*.git*' 'node_modules/*' 'package*.json'"
  },
  "keywords": [
    "chrome-extension",
    "bookmark",
    "semantic-search",
    "ai",
    "tensorflow",
    "nlp",
    "local-ai"
  ],
  "author": "AI Assistant",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/chrome-bookmark-semantic-search"
  },
  "bugs": {
    "url": "https://github.com/your-username/chrome-bookmark-semantic-search/issues"
  },
  "homepage": "https://github.com/your-username/chrome-bookmark-semantic-search#readme",
  "engines": {
    "chrome": ">=88.0"
  },
  "extensionConfig": {
    "manifestVersion": 3,
    "targetBrowser": "chrome",
    "features": [
      "semantic-search",
      "local-ai",
      "tensorflow-js",
      "universal-sentence-encoder"
    ]
  }
